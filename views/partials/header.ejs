<header>
    <% if (locals.login_User_ID == null ) { %>
        <%- include('../headers/withoutLogin.ejs') %>
    <% } else if (locals.login_User_ID == 'Admin') { %>
        <%- include('../headers/loginByAdmin.ejs') %>
    <% } else if (locals.login_User_ID == 'Agent') { %>
        <%- include('../headers/loginByAgent.ejs') %>
    <% } else if (locals.login_User_ID == 'Branch') { %>
        <%- include('../headers/loginByBranchManager.ejs') %>
    <% } else if (locals.login_User_ID == 'Customer') { %>
        <%- include('../headers/loginByCustomer.ejs') %>
    <% } else if (locals.login_User_ID == 'Owner') { %>
        <%- include('../headers/loginByOwner.ejs') %>
    <% } %>
</header>